{
  "root": "apps/gateway/app",
  "projectType": "application",
  "sourceRoot": "apps/gateway/app/src",
  "targets": {
    "build": {
      "executor": "nx-vue3-vite:build-app",
      "options": {
        "dist": "dist/apps/gateway/app"
      }
    },
    "serve": {
      "executor": "nx-vue3-vite:dev-server"
    },
    "e2e": {
      "executor": "nx-vue3-vite:cypress",
      "options": {
        "cypressConfig": "apps/gateway/app/cypress.json",
        "testingType": "component"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/gateway/app/**/*.{js,jsx,ts,tsx,vue}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/gateway/app"],
      "options": {
        "jestConfig": "apps/gateway/app/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
